<div class="filtered_coffee_shops">
    <% coffee_shops.each do |c| %>
      <%= image_tag c.image_url.to_s, height: "200", width: "250" %>
      <p>Name: <%= c.name %></p>
      <p>Rating: <%= c.rating %></p>
      <p>Location: <%= c.location[:display_address].join(",") %></p>
      <p>Phone: <%= c.phone %></p>
    <% end%>
  <hr>
</div>
